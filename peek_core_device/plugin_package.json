{
    "plugin": {
        "title": "Device Plugin",
        "packageName": "peek_core_device",
    "version":"0.0.1",
        "buildNumber": "#PLUGIN_BUILD#",
        "buildDate": "#BUILD_DATE#",
        "creator": "Synerty Pty Ltd",
        "website": "www.synerty.com"
    },
    "requiresServices": [
        "server",
        "storage",
        "admin",
        "client",
        "mobile",
        "agent"
    ],
    "server": {
    },
    "storage": {
        "alembicDir": "_private/alembic"
    },
    "admin": {
        "showHomeLink": true,
        "appDir": "_private/admin-app",
        "appModule": "device.module#DeviceModule",
        "moduleDir": "plugin-module",
    },
    "client": {
    },
    "mobile": {
        "showHomeLink": true,
        "appDir": "_private/mobile-app",
        "appModule": "device.module#DeviceModule",
        "assetDir": "_private/mobile-assets",
        "icon": "/assets/peek_core_device/icon.png",
        "moduleDir": "plugin-module"
    },
    "agent": {
    },
    "desktop": {
        "moduleDir": "plugin-module"
    }
